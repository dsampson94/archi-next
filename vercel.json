{
  "crons": [
    {
      "path": "/api/cron/process-documents",
      "schedule": "*/5 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/process-documents/route.ts": {
      "maxDuration": 60
    },
    "app/api/documents/route.ts": {
      "maxDuration": 60
    },
    "app/api/documents/[id]/retry/route.ts": {
      "maxDuration": 60
    },
    "app/api/debug/process-doc/route.ts": {
      "maxDuration": 60
    }
  }
}
